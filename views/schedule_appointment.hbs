<main class="main-content bgc-grey-100">
  <div id="mainContent">
    <div class="container-fluid">
      <h1 class="table-header text-center">Manage Schedule</h1>
      <div class="card shadow mb-4">
        <div class="card-header py-3">
          <h6 class="m-0 font-weight-bold text-primary">Post table</h6>
          <a href="/admin/manage_services_detail/add"><button type="button" class="btn btn-success ml-2"><i
                class="fas fa-plus-circle mr-1" style="color:#ffffff;"></i>Add Schedule</button></a>
        </div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered text-center" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>#</th>
                  <th>Customer Name</th>
                  <th>Phone Number</th>
                  <th>Schedule Book</th>
                  <th>Staff</th>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <th>#</th>
                  <th>Customer Name</th>
                  <th>Phone Number</th>
                  <th>Schedule Book</th>
                  <th>Staff</th>
                </tr>
              </tfoot>
              <tbody>
                {{#each this.booking}}
                <tr>
                  <td>{{id}}</td>
                  <td>{{customer_name}}</td>
                  <td>{{phone_number}}</td>
                  <td>{{schedule_booking}}</td>
                  <td>{{staff_work}}</td>
                </tr>
                {{/each}}

              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>