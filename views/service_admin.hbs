<!-- Page Heading -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <i class="fas fa-plus-circle"></i>
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Service Name</th>
                        <th>Created At</th>
                        <th>Updated At</th>  
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>#</th>
                        <th>Service Name</th>
                        <th>Created At</th>
                        <th>Updated At</th>                        
                    </tr>
                </tfoot>
                <tbody>
                    {{#each this.services}}
                    <tr>
                        <td>{{id}}</td>
                        <td>{{services_name}}</td>
                        <td>{{created_at}}</td>
                        <td>{{updated_at}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>